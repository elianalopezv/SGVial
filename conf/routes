# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /                           controllers.SegmentosController.segmentos
GET     /index                      controllers.SegmentosController.segmentos
GET     /segmentos                  controllers.SegmentosController.segmentos
GET     /detalle                    controllers.SegmentosController.detalle(id: Int)


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
